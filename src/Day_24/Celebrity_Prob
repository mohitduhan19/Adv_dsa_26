	// code here
    	Stack<Integer> s = new Stack<>();
    	for(int i=0;i<nums.length;i++)s.push(i);

    	while(s.size() > 1){
    	    int p1 = s.pop();
    	    int p2 = s.pop();

    	    if(nums[p1][p2] == 1)s.push(p2);
    	    else s.push(p1);

    	}
    	int pot = s.pop();
    	for(int i = 0;i < nums.length;i++){
    	    if(i != pot){
    	        if(nums[i][pot] == 0 || nums[pot][i] == 1)return -1;
    	    }
    	}
    	return pot;
    }