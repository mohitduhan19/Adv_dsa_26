   //Function to find the data of nth node from the end of a linked list.
    int size(Node head){
        Node curr = head;
        int s = 0;
        while(curr != null){
            s++;
            curr = curr.next;
        }
        return s;
    }
    int getNthFromLast(Node head, int n)
    {
        Node curr = head;
        int s = size(head);
        int val = s - n + 1 , x = 1;
        while(curr != null){
            if(x == val)return curr.data;
            curr = curr.next;
              x++;
        }
        return -1;
    }