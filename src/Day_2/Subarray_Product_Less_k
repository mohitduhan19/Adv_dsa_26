 public int numSubarrayProductLessThanK(int[] nums, int k) {
        int ans = 0 , l = 0  , pr =1;

        if(k == 0)return 0;

        for(int i = 0;i < nums.length;i++){
            pr *= nums[i];

            while(pr >= k){
                if(l > i)return ans;
                pr /= nums[l];
                l++;
            }


            if(pr== 0)return ans;

            ans =  ans + (i - l + 1);
        }
        return ans;
    }