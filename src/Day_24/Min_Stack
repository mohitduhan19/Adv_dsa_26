  Stack<Integer> alldata;
    Stack<Integer> mindata;
    public MinStack() {
        alldata = new Stack<>();
        mindata = new Stack<>();
    }

    public void push(int val) {
        alldata.push(val);
        if(mindata.size() == 0|| val <= mindata.peek()){
            mindata.push(val);
        }
    }

    public void pop() {
        int val = alldata.pop();
        if(val == mindata.peek())mindata.pop();
    }

    public int top() {
        return alldata.peek();
    }

    public int getMin() {
        return mindata.peek();
    }