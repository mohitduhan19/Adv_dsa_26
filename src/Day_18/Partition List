   public ListNode partition(ListNode head, int x) {
      ListNode lhe = new ListNode(0);
      ListNode l = lhe;
      ListNode rhe = new ListNode(0);
      ListNode r = rhe;

      while(head != null){
          if(head.val < x){
             l.next = head;
             l = head;
          }else{
             r.next = head;
             r = head;
          }
          head = head.next;
      }
      l.next = rhe.next;
      r.next = null;
      return lhe.next;
    }