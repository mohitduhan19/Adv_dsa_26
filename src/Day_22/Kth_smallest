  int count = 0 , ans = 0;
    public void inorder(TreeNode node, int k){
        if(node == null)return;

        inorder(node.left ,k);
        count++;

        if(count == k){
            ans = node.val;
        }

        inorder(node.right , k);
    }
    public int kthSmallest(TreeNode root, int k) {
        if(root == null)return 0;
        inorder(root , k);
        return ans;
    }