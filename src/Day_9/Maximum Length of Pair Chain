 public int findLongestChain(int[][] pairs) {
        Arrays.sort(pairs , (a , b) -> Integer.compare(a[1] , b[1]));

        int c = 0 , onGoing = Integer.MIN_VALUE;

        for(int i = 0;i < pairs.length;i++){
            if(onGoing < pairs[i][0]){
                c++;
                onGoing = pairs[i][1];
            }
        }
        return c;
    }